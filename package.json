{
  "name": "your-dedicated-marketer",
  "version": "0.1.0",
  "engines": {
    "node": ">=20 <23",
    "npm": ">=10"
  },
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "postbuild": "node scripts/check-client-secrets.mjs",
    "pages:build": "npx @cloudflare/next-on-pages",
    "pages:preview": "npm run pages:build && wrangler pages dev .vercel/output/static",
    "start": "next start",
    "lint": "eslint .",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,cjs,mjs,cts,mts,json,md,mdx,yml,yaml,css,scss}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,cjs,mjs,cts,mts,json,md,mdx,yml,yaml,css,scss}\"",
    "check:npm-registry": "node scripts/npm-registry-check.mjs",
    "audit:a11y": "node scripts/a11y-audit.mjs",
    "audit:lighthouse": "node scripts/lighthouse-audit.mjs",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "node scripts/ensure-vitest-coverage.mjs && vitest --coverage",
    "test:e2e": "playwright test"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@next/mdx": "15.5.2",
    "@sentry/nextjs": "10.32.1",
    "@upstash/ratelimit": "^2.0.7",
    "@upstash/redis": "^1.36.0",
    "clsx": "^2.1.1",
    "gray-matter": "^4.0.3",
    "lucide-react": "^0.468.0",
    "next": "15.5.2",
    "next-mdx-remote": "^5.0.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-hook-form": "^7.54.2",
    "reading-time": "^1.5.0",
    "rehype-pretty-code": "^0.14.1",
    "rehype-slug": "^6.0.0",
    "remark-gfm": "^4.0.1",
    "resend": "6.6.0",
    "shiki": "^3.20.0",
    "tailwind-merge": "^2.6.0",
    "zod": "4.3.5"
  },
  "devDependencies": {
    "@cloudflare/next-on-pages": "^1.13.16",
    "@playwright/test": "^1.49.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/mdx": "^2.0.13",
    "@types/node": "^22.10.5",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.2",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.39.2",
    "eslint-config-next": "15.1.4",
    "eslint-plugin-react": "^7.37.5",
    "globals": "^17.0.0",
    "jsdom": "^27.4.0",
    "postcss": "^8.4.49",
    "prettier": "^3.4.2",
    "sharp": "^0.34.5",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.52.0",
    "vitest": "^4.0.16",
    "wrangler": "^4.57.0"
  }
}
